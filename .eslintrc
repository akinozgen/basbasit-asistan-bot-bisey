{
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:node/recommended"],
  "plugins": ["prettier", "node"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 100
      }
    ]
  },
  "env": {
    "node": true
  },
  "globals": {
    "test": "readonly",
    "expect": "readonly",
    "describe": "readonly"
  }
}
